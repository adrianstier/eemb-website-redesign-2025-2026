{
  "collectionName": "components_shared_dei_initiatives",
  "info": {
    "displayName": "DEI Initiative",
    "icon": "lightbulb",
    "description": "DEI initiative or program"
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 200
    },
    "description": {
      "type": "richtext",
      "maxLength": 2000
    },
    "status": {
      "type": "enumeration",
      "enum": ["Active", "Planned", "Completed"],
      "default": "Active"
    },
    "startDate": {
      "type": "date"
    },
    "link": {
      "type": "string",
      "regex": "^https?://.*"
    }
  }
}